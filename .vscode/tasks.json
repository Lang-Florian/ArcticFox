{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++: g++ build active file",
      "command": "g++",
      "args": [
        "-fdiagnostics-color=always",
        "-g",
        "main.cpp",
        "-o",
        "${fileDirname}/main",
        "-std=c++23",
        "-fconstexpr-ops-limit=100000000000",
        "-Ofast",
        "-march=native",
        "-flto",
        "-fno-signed-zeros",
        "-frename-registers",
        "-funroll-loops"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Task generated by Debugger."
    }
  ],
  "version": "2.0.0"
}